source "https://rubygems.org"
# Gems included in the :jekyll-plugins group are activated regardless of the --safe mode setting
# remark: because of GitHub Pages, plugins used on GH Pages need to appear in both Gemfile and _config.yml 
group :jekyll_plugins do
	gem "github-pages"
	gem "jekyll-sitemap"
	gem "jekyll-seo-tag"
  	gem "jekyll-sitemap" 
  	gem "jekyll-minifier"
end

gem "kramdown", ">= 2.3.0"

gem "webrick", "~> 1.7"
